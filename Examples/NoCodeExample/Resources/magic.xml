                <Parameter paramID="Q1freq" Control="Frequency"/>
                <Parameter paramID="Q1gain" Control="Gain"/>
                <Parameter paramID="Q1quality" Control="Quality"/>
            </IIR>
            <Gain Name="Master" Input="Q1">
                <Parameter paramID="output" Control="Gain"/>
            </Gain>
            <LevelMeter Name="outputMeter" Input="Gain"/>
            <Analyzer Name="output" Input="Gain"/>
        </Program>
